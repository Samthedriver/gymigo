<div class="container">
<%= form_for @workout do |f| %>
  <div class="form-group">
  <%= f.label :start_time %>
  <%= f.text_field :start_time %><br>
</div>

<div class="form-group">
  <%= f.label :end_time, class: "form-text" %>
  <%= f.text_field :end_time, class: "form-control" %><br>
</div>

  <div class="form-group">
  <%= f.label :description, class: "form-text" %>
  <%= f.text_area :description, class: "form-control"  %><br>
</div>

  <div class="form-group">
  <%= f.label :gym, class: "form-text" %>
  <%= f.collection_select :gym_id, Gym.all, :id, :name, class: "form-control" %><br>
</div>

  <div class="form-group">

  <%= f.submit :'Create Workout', class: "btn btn-primary" %>
  </div>
</div>

<% end %>
</div>
